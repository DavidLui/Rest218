<h1>Listing riceincasseroles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Img</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @riceincasseroles.each do |riceincasserole| %>
      <tr>
        <td><%= riceincasserole.name %></td>
        <td><%= riceincasserole.price %></td>
        <td><%= riceincasserole.img %></td>
        <td><%= link_to 'Show', riceincasserole %></td>
        <td><%= link_to 'Edit', edit_riceincasserole_path(riceincasserole) %></td>
        <td><%= link_to 'Destroy', riceincasserole, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Riceincasserole', new_riceincasserole_path %>
