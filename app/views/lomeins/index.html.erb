<h1>Listing lomeins</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Img</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lomeins.each do |lomein| %>
      <tr>
        <td><%= lomein.name %></td>
        <td><%= lomein.price %></td>
        <td><%= lomein.img %></td>
        <td><%= link_to 'Show', lomein %></td>
        <td><%= link_to 'Edit', edit_lomein_path(lomein) %></td>
        <td><%= link_to 'Destroy', lomein, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lomein', new_lomein_path %>
